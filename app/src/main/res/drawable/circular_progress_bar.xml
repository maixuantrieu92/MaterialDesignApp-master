<?xml version="1.0" encoding="UTF-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="120"
    android:pivotX="100%"
    android:pivotY="100%"
    android:toDegrees="140">
    <item android:id="@android:id/background">
        <shape
            android:innerRadiusRatio="5"
            android:shape="ring"
            android:useLevel="true"
            android:angle="0"
            android:type="sweep"
            android:thicknessRatio="8.0">
            <solid android:color="#49494e"/>
        </shape>

    </item>
    <item android:id="@android:id/progress">
        <rotate
            android:fromDegrees="220"
            android:toDegrees="220">
            <shape
                android:innerRadiusRatio="5"
                android:shape="ring"
                android:useLevel="true"
                android:angle="0"
                android:type="sweep"
                android:thicknessRatio="8.0">
                <solid android:color="#f1b809"/>
            </shape>
        </rotate>
    </item>
</layer-list>